
# CRM HTACCESS - PRODUCTION
RewriteEngine On
RewriteBase /

# IMPORTANT: Do not rewrite requests for the API
RewriteCond %{REQUEST_URI} ^/admission-api [NC]
RewriteRule .* - [L]

# Normal file handling
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# SPA Fallback
RewriteRule ^ index.html [L]
